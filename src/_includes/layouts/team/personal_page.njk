{% extends "layouts/base.njk" %}

{% block page_header %}
<div class="container-xxl page-header">
    <div class="container">
        <div class="row g-5 align-items-center">
            <div class="col-md-6 text-center text-md-start">
                


                <div class="member_profile_modal__cover d-flex align-items-center">
                                <div class="flex-shrink-0">
                                <img class="member_profile_modal___profile_img" src="{{profile_img}}"
                                    alt="" class="img-fluid">
                                </div>
                                <div class="member_profile_modal__headers ms-3">
                                <p class="align-items-center">
                                    <h5>{{full_name}}, <small>{{credentials}}</small> </h5>
                                    
                                    
                                </p>
                                
                                </div>
                                
                                </div>
                            </div>
                
            </div>
            <div class="col-md-6 text-center text-lg-start">
               
            </div>
        </div>
    </div>
</div>
{% endblock %}

{% block page_main_content %}
    <div class="content">
        <div class="col-md-8 mx-auto mt-4" style="text-align: left;">
            {{ content | safe }}

        </div>
    </div>
{% endblock %}